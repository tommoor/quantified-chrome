{
	"name": "Quantified Chrome",
	"version": "0.0.1",
	"description": "Keep track of your browsing habits",
	"background": {
	    "scripts": [
            "js/pouchdb-2.2.2.min.js",
            "js/underscore.js", 
            "js/quantified.js",
            "js/browseraction.js"
        ]
	},
    "browser_action": {
      "default_icon": {
        "19": "images/icon19.png",
        "38": "images/icon38.png" 
      },
      "default_title": "Quantified Chrome"
    },
    "manifest_version": 2,
    "permissions": [
        "tabs"
    ]
}